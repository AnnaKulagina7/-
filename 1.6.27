x = int(input('Введите год - '))
a = x % 19
b = x % 4
c = x % 7
k = x // 100
print('k = ', k)
p = ((13 + 8*k) // 25)
q = k // 4
M = (15 - p + k - q) % 30
N = (4 + k - q) % 7
d = (19*a + M) % 30
e = (2*b + 4*c + 6*d + N) % 7
if 28 == d == 29 and e == 6 and (11*M + 11) % 30 < 19:
    z1 = 22 + d + e
    print('В этом году Пасха будет праздноваться ', z1, 'марта')
else:
     z2 = d + e - 9
     print('В этом году Пасха будет праздноваться ', z2, 'апреля')
